<!DOCTYPE module SYSTEM "module.dtd">

<module name="fs_landing" dir="fs_landing">
  <doc>
    <description>
      Flat spin controller
    </description>
  </doc>

  <settings>
    <dl_settings name="Flat Spin Landing">
      <dl_settings name="Feed Forward Settings">
        <dl_setting min="0.1" max="10" step="0.1" shortname="FF Duration" var="ff_duration" type="float" module="fs_landing/feed_forward" handler="ff_duration_handler" />
        <dl_setting min="0" max="9600" step="50" shortname="THR_L strength" var="thr_l_value" type="float" module="fs_landing/feed_forward" handler="thr_l_handler" />
        <dl_setting min="0" max="9600" step="50" shortname="THR_R strength" var="thr_r_value" type="float" module="fs_landing/feed_forward" handler="thr_r_handler" />
        <dl_setting min="0" max="9600" step="50" shortname="ELEV_L strength" var="elev_l_value" type="float" module="fs_landing/feed_forward" handler="elev_l_handler" />
        <dl_setting min="0" max="9600" step="50" shortname="ELEV_R strength" var="elev_r_value" type="float" module="fs_landing/feed_forward" handler="elev_l_handler" />
      </dl_settings>
    </dl_settings>
  </settings>

  <header>
    <file name="fs_landing.h"/>
  </header>

  <init fun="fs_landing_init()"/>
  <periodic fun="fs_landing_run()"/>

  <makefile>
    <file name="fs_landing.c"/>
    <file name="feed_forward.c"/>
  </makefile>

</module>

