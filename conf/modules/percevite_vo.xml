<!DOCTYPE module SYSTEM "module.dtd">
<module name="percevite_vo" dir="percevite_vo">
  <doc>
    <description>Percevite's Velocity Obsctacle for GPS and avoid</description>
  </doc>
  <header>
    <file name="percevite_vo.h"/>
  </header>
  <init fun="percevite_vo_init()"/>
  <!-- Joystick's ATT mode is autorun -->
  <periodic fun="percevite_vo_periodic()" freq="0.1" autorun="TRUE"/>
  <periodic fun="lateral_pos_ctrl()" freq="128.0" autorun="FALSE"/>
  <makefile>
    <file name="percevite_vo.c"/>
  </makefile>
</module>

<!-- Assume both robots to be placed at ref in start
    Then manually start VO after -7,-7 and 7,-7 -->