      <exception cond="GetAltRef() + 10 > GetPosAlt()" deroute="flare"/>
      <go from="AF" hmode="route" vmode="glide" wp="TD"/>
    </block>
    <block name="flare">
      <go approaching_time="0" from="AF" hmode="route" throttle="0.0" vmode="throttle" wp="TD"/>
      <attitude roll="0.0" throttle="0.0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="ReturnCenterPointNoThrottle">
      <exception cond="And(!(radio_control.status == RC_REALLY_LOST), And(InsideGeoFence(GetPosX(), GetPosY()), And(MoreThan(GetPosAlt(), GetAltRef() + 20), LessThan(GetPosAlt(), GetAltRef() + 80))))" deroute="Standby"/>
      <set value="1" var="autopilot.kill_throttle"/>
      <circle radius="nav_radius" wp="CenterPoint" vmode="throttle" throttle="0.0"/>
    </block>
  </blocks>
</flight_plan>
