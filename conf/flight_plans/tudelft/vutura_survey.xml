<!DOCTYPE flight_plan SYSTEM "../flight_plan.dtd">

<flight_plan alt="100" ground_alt="0" lat0="52.16810052554008" lon0="4.412674313128298" max_dist_from_home="1500" name="Basic" security_height="50">
  <header>
#include "subsystems/datalink/datalink.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0" y="0"/>
    <!--waypoint name="CenterPoint" x="49.5" y="-100.1"/-->
    <waypoint name="1" x="10.1" y="189.9"/>
    <waypoint name="2" x="132.3" y="139.1"/>
