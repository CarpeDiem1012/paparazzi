    <waypoint name="S2" x="274.4" y="209.5"/>
    <waypoint alt="215.0" name="AF" x="177.4" y="45.1"/>
    <waypoint alt="185.0" name="TD" x="28.8" y="57.0"/>
    <waypoint name="_BASELEG" x="168.8" y="-13.8"/>
    <waypoint name="CLIMB" x="50" y="-50"/>
    <waypoint name="CenterPoint" lat="52.16882159944184" lon="4.417506194127447"/>
    <waypoint name="ROUTE_00" lat="52.167153758107176" lon="4.414723323126365" height="100"/>
    <waypoint name="ROUTE_01" lat="52.16911055333959" lon="4.419393630221458" height="100"/>
